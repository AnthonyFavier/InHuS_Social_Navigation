<launch>
  <arg name="ns" default="human1"/>
  <arg name="localize" default="fake"/>
  <arg name="num_agents" default="3"/>
  <arg name="node_start_delay" default="2.0"/>
  <arg name="map_name" default="passage_hri"/>
  <!-- <arg name="map_name" default="corridor_hri"/> -->
  <!-- <arg name="map_name" default="narr_corridor_hri"/> -->
  <!-- <arg name="map_name" default="laas_adream"/> -->

<!-- MAP SERVERS -->
  <!-- start map_server with laas_adream map -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/laas_adream/passage_hri.yaml" if="$(eval 'passage_hri'==arg('map_name'))"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/laas_adream/corridor_hri.yaml" if="$(eval 'corridor_hri'==arg('map_name'))"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/laas_adream/narr_corridor_hri.yaml" if="$(eval 'narr_corridor_hri'==arg('map_name'))"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/laas_adream/laas_adream.yaml" if="$(eval 'laas_adream'==arg('map_name'))"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/maze/maze.yaml" if="$(eval 'maze'==arg('map_name'))"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find inhus_navigation)/maps/open_space/open_space.yaml" if="$(eval 'open_space'==arg('map_name'))"/>
  <!-- static map server -->
  <include file="$(find inhus_navigation)/launch/inhus_navigation_launch/include/map_static.launch">
    <arg name="ns" value="$(arg ns)"/>
    <arg name="map_name" value="$(arg map_name)"/>
  </include>
  <!-- pov map server -->
  <include file="$(find inhus_navigation)/launch/inhus_navigation_launch/include/map_pov.launch">
    <arg name="ns" value="$(arg ns)"/>
    <arg name="map_name" value="$(arg map_name)"/>
  </include>

  <include file="$(find inhus_navigation)/launch/cohan_nav/include/robot_only.launch">
    <arg name="node_start_delay" value="$(arg node_start_delay)" />
    <arg name="localize" value="$(arg localize)"/>
    <arg name="num_agents" value="$(arg num_agents)"/>
  </include>

  <include file="$(find inhus_navigation)/launch/cohan_nav/include/agent_with_ns.launch">
    <arg name="ns" value="$(arg ns)"/>
    <arg name="node_start_delay" value="$(arg node_start_delay)" />
    <arg name="localize" value="$(arg localize)"/>
    <arg name="num_agents" value="$(arg num_agents)"/>
  </include>

</launch>
