define floorplan model
(
  # sombre, sensible, artistic
  color "gray30"

  # most maps will need a bounding box
  boundary 1

  gui_nose 0
  gui_grid 0

  gui_outline 1
  gripper_return 0
  fiducial_return 0
  ranger_return 1
)

# set the resolution of the underlying raytrace model in meters
resolution 0.01

interval_sim 20  # simulation timestep in milliseconds

window
(
  size [ 745.000 448.000 ]
  rotate [ 0.000 0.000 ]
  scale 28.806
)

# load an environment bitmap
floorplan
(
  name "laas"
  bitmap "maps/laas_adream.pgm"
  size [19.2 41.6 2.0 ]
  pose [4.606 10.744 0.000 0.000 ]
)

# throw in a robot (always the robot first)
pr2( pose [ 2.0 2.0 0 0 ] name "pr2" color "gray")
human( pose [ 3.0 1.0 0 0 ] name "human1" color "blue")
human( pose [ 7.5 5.0 0 90.0 ] name "human2" color "green")
