define human position
(
  bitmap "images/human_base.png"
  size [0.60 0.60 0.25]
  origin [0.0 0 0 0]
  gui_nose 1
  localization "gps"
  drive "omni"
  laser_sensor(pose [ 0.275 0.000 -0.150 0.000 ])

    cap_b(pose [0.05 0.0 1.25 0 ] color "brown")
