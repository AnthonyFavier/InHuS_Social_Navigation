<launch>

	<arg name="map_name" default="laas_adream"/>

	<node name="map_server" pkg="map_server" type="map_server" ns="human" args="$(find inhus_navigation)/maps/laas_adream/laas_adream.yaml" if="$(eval 'laas_adream'==arg('map_name'))">
		<remap from="map" to="map_static"/>
	</node>
	<node name="map_server" pkg="map_server" type="map_server" ns="human" args="$(find inhus_navigation)/maps/laas_adream_big/laas_adream_big.yaml" if="$(eval 'laas_adream_big'==arg('map_name'))">
		<remap from="map" to="map_static"/>
	</node>
  <node name="map_server" pkg="map_server" type="map_server" ns="human" args="$(find inhus_navigation)/maps/maze/maze.yaml" if="$(eval 'maze'==arg('map_name'))">
		<remap from="map" to="map_static"/>
	</node>
  <node name="map_server" pkg="map_server" type="map_server" ns="human" args="$(find inhus_navigation)/maps/open_space/open_space.yaml" if="$(eval 'open_space'==arg('map_name'))">
		<remap from="map" to="map_static"/>
	</node>

</launch>
